{
  "name": "mindgap",
  "private": true,
  "scripts": {
    "clean-dist": "rm -f -r -d dist",
    "build": "npm run clean-dist && webpack",
    "start": "npm run start:watch",
    "start:watch": "webpack-dev-server --hot",
    "format:check": "prettier --config .prettierrc.yaml --list-different '{src,test}/**/*.{ts,tsx,js,jsx,html}'",
    "format": "prettier --config .prettierrc.yaml --write '{src,test}/**/*.{ts,tsx,js,jsx,html}'",
    "lint:check": "eslint '{src,test}/**/*.{ts,tsx}'",
    "lint": "eslint --fix '{src,test}/**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@types/luxon": "^1.25.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "fork-ts-checker-webpack-plugin": "^6.0.0",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "^2.1.2",
    "react-router-dom": "^5.2.0",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "bulma": "^0.9.1",
    "immer": "^7.0.14",
    "lazy-space": "^0.3.10",
    "luxon": "^1.25.0",
    "react": "^17.0.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  }
}
